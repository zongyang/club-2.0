extends ../layout
block content
    
    #carousel.carousel.slide(data-ride='carousel')
      // Indicators
      ol.carousel-indicators
        -for(var i=0;i<carousels.length;i++)
          li(data-target='#carousel', data-slide-to='#{i}')
      .carousel-inner(role='listbox')
        each carousel in carousels
          .item
            img.first-slide(src='#{carousel.img}', alt='First slide')
            .container
              .carousel-caption
                h1 #{carousel.name}
                p #{carousel.content}
                  
                p
                  a.btn.btn-primary(href='#{carousel.url}', role='button') #{carousel.name}
        
      a.left.carousel-control(href='#carousel', role='button', data-slide='prev')
        span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
        span.sr-only Previous
      a.right.carousel-control(href='#carousel', role='button', data-slide='next')
        span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
        span.sr-only Next

